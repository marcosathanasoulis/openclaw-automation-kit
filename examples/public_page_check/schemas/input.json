{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["url"],
  "additionalProperties": false,
  "properties": {
    "url": {
      "type": "string",
      "format": "uri"
    },
    "keyword": {
      "type": "string",
      "default": "mental"
    },
    "task": {
      "type": "string",
      "enum": ["keyword_count", "headlines", "summary"],
      "default": "keyword_count"
    }
  }
}
