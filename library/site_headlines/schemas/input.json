{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["url"],
  "additionalProperties": false,
  "properties": {
    "url": {
      "type": "string",
      "format": "uri"
    },
    "max_items": {
      "type": "integer",
      "minimum": 1,
      "maximum": 20,
      "default": 8
    }
  }
}
